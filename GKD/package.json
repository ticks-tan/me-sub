{
  "name": "@gkd-kit/subscription-template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --cache --write ./**/*.{js,cjs,mjs,ts,jsx,tsx,json}",
    "lint": "eslint --cache --fix ./**/*.{js,cjs,mjs,ts,jsx,tsx}",
    "check": "tsc --noEmit && tsx ./scripts/check.ts",
    "build": "tsc --noEmit && tsx ./scripts/build.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@gkd-kit/api": "0.0.5",
    "@gkd-kit/define": "0.0.1",
    "@gkd-kit/tools": "0.2.0",
    "@types/node": "20.12.10",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-define-config": "2.1.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "json5": "2.2.3",
    "prettier": "3.2.5",
    "tsx": "4.9.3",
    "typescript": "5.4.5"
  },
  "volta": {
    "node": "20.12.1"
  },
  "packageManager": "pnpm@9.1.0",
  "engineStrict": true,
  "engines": {
    "node": ">=20"
  }
}
